{
  "name": "dots-game",
  "version": "0.0.1",
  "description": "html5 version of a classic paper-based game",
  "main": "src/app.js",
  "scripts": {
    "lint": "eslint ./src/",
    "test": "mocha --compilers js:./src/test/_compiler.js --require ./src/test/_setup.js --reporter spec ./src/test",
    "dev": "webpack-dev-server --progress --colors",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "export NODE_ENV=production && webpack --progress --colors --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksmarkh/dots-game.git"
  },
  "author": "Oleksii Markhovskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oleksmarkh/dots-game/issues"
  },
  "homepage": "https://github.com/oleksmarkh/dots-game",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.2.0",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "hidpi-canvas": "^1.0.10",
    "lodash": "^4.17.4"
  }
}
