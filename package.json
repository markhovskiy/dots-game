{
  "name": "dots-game",
  "version": "0.0.1",
  "description": "html5 version of a classic paper-based game",
  "main": "src/app.js",
  "scripts": {
    "lint": "eslint ./src/",
    "test": "mocha --compilers js:./src/test/_compiler.js --require ./src/test/_setup.js --reporter spec ./src/test",
    "server:dev": "webpack-dev-server --progress",
    "clean": "rm -rf dist/*",
    "prebuild": "yarn run clean && yarn run lint && yarn run test",
    "build": "export NODE_ENV=production && webpack --progress --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oleksmarkh/dots-game.git"
  },
  "author": "Oleksii Markhovskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oleksmarkh/dots-game/issues"
  },
  "homepage": "https://github.com/oleksmarkh/dots-game",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "mocha": "^3.4.2",
    "sinon": "^2.4.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "hidpi-canvas": "^1.0.10"
  }
}
